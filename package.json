{
  "name": "eslint-config-aesthetic-coder",
  "version": "1.2.6",
  "private": false,
  "description": "Eslint configuration of AesthetiCoder for personal projects and more",
  "author": "AesthetiCoder",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "eslintplugin",
    "eslint-plugin",
    "eslint-rules",
    "eslint-shareable-configs",
    "linter",
    "linter-eslint",
    "linter-config",
    "style-guide",
    "styleguide",
    "typescript",
    "clean-code",
    "javascript",
    "code-style"
  ],
  "homepage": "https://github.com/AesthetiCoder/eslint-config-aesthetic-coder",
  "bugs": {
    "url": "https://github.com/AesthetiCoder/eslint-config-aesthetic-coder/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AesthetiCoder/eslint-config-aesthetic-coder.git"
  },
  "files": [
    "./dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./typescript": {
      "require": "./dist/typescript.js",
      "import": "./dist/typescript.mjs",
      "types": "./dist/typescript.d.ts"
    },
    "./qwik": {
      "require": "./dist/qwik.js",
      "import": "./dist/qwik.mjs",
      "types": "./dist/qwik.d.ts"
    },
    "./jsx_a11y": {
      "require": "./dist/jsx_a11y.js",
      "import": "./dist/jsx_a11y.mjs",
      "types": "./dist/jsx_a11y.d.ts"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "^18.16.0",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">=8.6.1"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "git config core.hooksPath ./script/git_hook",
    "lint": "eslint --ext .js, .ts . --fix",
    "build": "rimraf dist && tsc",
    "type-check": "tsc --pretty --noEmit",
    "release": "bash ./script/module/release.sh"
  },
  "devDependencies": {
    "eslint": "^8.43.0",
    "eslint-plugin-qwik": "^1.1.5",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "eslint": "^7.32.0 || ^8.2.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint-plugin-etc": "^2.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-perfectionist": "^1.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-jsx-a11y": "^6.7.1"
  }
}