{
  "name": "eslint-config-elegant-coding",
  "version": "4.0.5",
  "private": false,
  "description": "⚙️ Eslint configuration for help you to identify and fix these errors before they cause problems in your code. It can also help you to develop good coding habits that will make your code easier to maintain in the long run.",
  "author": "Luis Monsalve <TheElegantCoding>",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "eslintplugin",
    "eslint-plugin",
    "eslint-rules",
    "eslint-shareable-configs",
    "linter",
    "linter-eslint",
    "linter-config",
    "style-guide",
    "styleguide",
    "typescript",
    "clean-code",
    "javascript",
    "code-style"
  ],
  "type": "module",
  "files": [
    "./dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": "^18.16.0",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">=8.6.1"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsup --format esm,cjs --clean --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch & eslint-flat-config-viewer",
    "postinstall": "git config core.hooksPath ./script/git_hook",
    "type-check": "tsc --pretty --noEmit",
    "lint": "pnpm lint-eslint",
    "lint-eslint": "eslint . --fix "
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "devDependencies": {
    "@types/eslint": "8.44.8",
    "eslint": "8.55.0",
    "eslint-flat-config-viewer": "0.1.3",
    "tsup": "8.0.1",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "1.5.1",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "astro-eslint-parser": "0.16.0",
    "eslint-plugin-astro": "0.30.0",
    "eslint-plugin-github": "4.10.1",
    "eslint-plugin-i": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-n": "16.4.0",
    "eslint-plugin-perfectionist": "2.5.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-qwik": "1.3.0",
    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-solid": "0.13.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-unicorn": "49.0.0",
    "globals": "^13.24.0"
  }
}
