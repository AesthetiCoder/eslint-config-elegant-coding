{
  "name": "eslint-config-aesthetic-coder",
  "version": "2.0.9",
  "private": false,
  "description": "Eslint configuration of AesthetiCoder for personal projects and more",
  "author": "AesthetiCoder",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "eslintplugin",
    "eslint-plugin",
    "eslint-rules",
    "eslint-shareable-configs",
    "linter",
    "linter-eslint",
    "linter-config",
    "style-guide",
    "styleguide",
    "typescript",
    "clean-code",
    "javascript",
    "code-style"
  ],
  "homepage": "https://github.com/AesthetiCoder/eslint-config-aesthetic-coder",
  "bugs": {
    "url": "https://github.com/AesthetiCoder/eslint-config-aesthetic-coder/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AesthetiCoder/eslint-config-aesthetic-coder.git"
  },
  "files": [
    "./dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./typescript": {
      "require": "./dist/typescript.js",
      "import": "./dist/typescript.mjs",
      "types": "./dist/typescript.d.ts"
    },
    "./qwik": {
      "require": "./dist/qwik.js",
      "import": "./dist/qwik.mjs",
      "types": "./dist/qwik.d.ts"
    },
    "./jsx_a11y": {
      "require": "./dist/jsx_a11y.js",
      "import": "./dist/jsx_a11y.mjs",
      "types": "./dist/jsx_a11y.d.ts"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "^18.16.0",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">=8.6.1"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "git config core.hooksPath ./script/git_hook",
    "type-check": "tsc --pretty --noEmit",
    "lint": "pnpm lint-eslint && pnpm lint-markdown",
    "lint-eslint": "eslint --color --ext .js,.jsx,.ts,.tsx --fix ",
    "lint-markdown": "markdownlint-cli2 --fix '**/*.md' '#node_modules'",
    "build": "pnpm type-check && pnpm lint && rimraf dist && tsc"
  },
  "devDependencies": {
    "eslint": "8.52.0",
    "markdownlint-cli2": "0.10.0",
    "rimraf": "5.0.5",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "eslint": "8.52.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "eslint-plugin-etc": "2.0.3",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-perfectionist": "2.2.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-qwik": "1.2.16",
    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-sonarjs": "0.22.0",
    "eslint-plugin-unicorn": "49.0.0"
  }
}