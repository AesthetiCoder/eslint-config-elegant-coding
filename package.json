{
  "name": "eslint-config-elegant-coding",
  "version": "3.1.11",
  "private": false,
  "description": "⚙️ Eslint configuration for help you to identify and fix these errors before they cause problems in your code. It can also help you to develop good coding habits that will make your code easier to maintain in the long run.",
  "author": "Luis Monsalve <TheElegantCoding>",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "eslintplugin",
    "eslint-plugin",
    "eslint-rules",
    "eslint-shareable-configs",
    "linter",
    "linter-eslint",
    "linter-config",
    "style-guide",
    "styleguide",
    "typescript",
    "clean-code",
    "javascript",
    "code-style"
  ],
  "files": [
    "./dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./typescript": {
      "require": "./dist/typescript.js",
      "import": "./dist/typescript.mjs",
      "types": "./dist/typescript.d.ts"
    },
    "./qwik": {
      "require": "./dist/qwik.js",
      "import": "./dist/qwik.mjs",
      "types": "./dist/qwik.d.ts"
    },
    "./jsx_a11y": {
      "require": "./dist/jsx_a11y.js",
      "import": "./dist/jsx_a11y.mjs",
      "types": "./dist/jsx_a11y.d.ts"
    },
    "./astro": {
      "require": "./dist/astro.js",
      "import": "./dist/astro.mjs",
      "types": "./dist/astro.d.ts"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "^18.16.0",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">=8.6.1"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "git config core.hooksPath ./script/git_hook",
    "type-check": "tsc --pretty --noEmit",
    "lint": "pnpm lint-eslint",
    "lint-eslint": "eslint --color --ext .js,.jsx,.ts,.tsx --fix ",
    "build": "pnpm type-check && pnpm lint && rimraf dist && tsc"
  },
  "devDependencies": {
    "eslint": "8.53.0",
    "rimraf": "5.0.5",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "1.1.0",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "eslint-plugin-astro": "0.30.0",
    "eslint-plugin-etc": "2.0.3",
    "eslint-plugin-github": "4.10.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-perfectionist": "2.3.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-qwik": "1.2.17",
    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-solid": "0.13.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-unicorn": "49.0.0"
  }
}
